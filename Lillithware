-- Load the Orion Library
local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()

-- Create the main window
local Window = OrionLib:MakeWindow({
    Name = "Lillithware (Id Changer)",
    HidePremium = false,
    SaveConfig = true,
    ConfigFolder = "Universal Id Changer",
    IntroText = "Lilithware"
})

-- Create tabs
local MainTab = Window:MakeTab({
    Name = "Main",
    Icon = "rbxassetid://4483345998",
    PremiumOnly = false
})

-- Add a Button for Id changer
MainTab:AddButton({
    Name = "Id Changer",
    Callback = function()
        -- Insert the code for Id changer here
        loadstring(game:HttpGet("https://raw.githubusercontent.com/lillithOfPortals/Id-changer/main/Ho"))()
    end
})

-- Initialize the Orion Library
OrionLib:Init()
